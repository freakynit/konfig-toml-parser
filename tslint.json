{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "no-console": false
  },
  "rules": {
    "quotemark": [true, "single", "avoid-escape"],
    "trailing-comma": false,
    "max-line-length": [false],
    "no-console": false,
    "only-arrow-functions": [false],
    "typedef": [false],
    "prefer-const": [false]
  },
  "rulesDirectory": []
}
